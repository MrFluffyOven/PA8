# logical
/system         ext4              flags=display="System"              wait,logical,first_stage_mount
/system_ext     ext4              flags=display="System_Ext"          wait,logical,first_stage_mount
/vendor         ext4              flags=display="Vendor"              wait,logical,first_stage_mount
/product        ext4              flags=display="Product"             wait,logical,first_stage_mount

# Syslinked
/system_image   emmc           /dev/block/dm-0              flags=wipingui;flashimg=1;backup=1;display="System Image"
/system_ext     emmc         /dev/block/dm-1              flags=display="System_Ext"
/vendor_image   emmc          /dev/block/dm-2              flags=wipingui;flashimg=1;backup=1;display="Vendor Image"
/product_image  emmc            /dev/block/dm-3              flags=wipingui;flashimg=1;backup=1;display="Product Image"


/boot                emmc      /dev/block/by-name/boot              flags=display="Boot";flashimg=1;backup=1
/recovery            emmc      /dev/block/by-name/recovery          flags=display="Recovery";flashimg=1;backup=1
/metadata            ext4      /dev/block/by-name/metadata          flags=display="Metadata";backup=1
/cache               ext4      /dev/block/by-name/cache             flags=display="Cache";backup=1
/data                f2fs      /dev/block/by-name/userdata          flags=display="Data";backup=1;wipingui
/prism               ext4      /dev/block/by-name/prism             flags=display="Prism";flashimg=1
/optics              ext4      /dev/block/by-name/optics            flags=display="Optics"

/external_sd              sdfat     /dev/block/mmcblk1p1                 flags=display=sdcard
/usb_otg